<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="l10n_ua_uktz_res_config_settings_form" model="ir.ui.view">
        <field name="name">res.config.settings.form (l10n_ua_uktz)</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('settings')]" position="inside">
                <div class="app_settings_block" data-string="Uktzed" string="Uktzed" data-key="uktzed">
                <h2>Uktzed</h2>
                    <div class="row mt16 o_settings_container">
                        <div class="col-xs-12 col-md-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <div class="content-group">
                                    <div class="mt16">
                                        <button name="upload_uktz_wizard" class="oe_highlight" string="Upload Uktzed" type="object"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</odoo>
