<odoo>
    <record id="kw_invoice_doc_base_account_move_form" model="ir.ui.view">
        <field name="name">account.move.form (kw_invoice_doc_base)</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Responsible person">
                    <group>
                        <field name="kw_responsible_person"/>
                        <field name="kw_city_of_assembly"/>
                        <field name="kw_contract"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
